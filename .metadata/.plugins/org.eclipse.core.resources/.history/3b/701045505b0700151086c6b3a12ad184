
public class Test {
	private String name;
	private String surname;
	private int age;
	
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getSurname() {
		return surname;
	}
	public void setSurname(String surname) {
		this.surname = surname;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	public int hashCode(){
		String s1;
		s1 = this.name + " : " + this.surname + " : " + this.age;
		return s1.hashCode();
	}
	
	
}
